{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Https1003": {
        "Url": "https://*:1003",
        "Certificate": {
          "AllowInvalid": true
        }
      }
    }
  },
  "Grpc": {
    // 主机名
    "Host": "localhost",
    // 端口
    "Port": 1002,
    // 服务端证书文件，此证书不含有私钥
    "ServerCertificateFile": "certs/server.crt",
    // 客户端证书文件
    "ClientCertificateFile": "certs/my.crt",
    // 客服端证书文件私钥
    "ClientCertificateKeyFile": "certs/my.key"
  },
  "AllowedHosts": "*"
}
